{
  "version": 2,
  "alias": "soudyshirt.jacobford.com",
  "functions": {
    "**/*.php": {
      "runtime": "vercel-php@0.3.1"
    }
  },
  "routes": [
    {
      "src": ".*",
      "headers": {
        "Cache-Control": "s-maxage=604800, stale-while-revalidate"
      },
      "continue": true
    },
    {
      "src": "/",
      "dest": "/api/index.php"
    },
    {
      "src": "(.*).php",
      "dest": "/api/$1.php"
    }
  ],
  "github": {
    "silent": true
  }
}
